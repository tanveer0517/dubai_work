<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--Saas Client Registration Template View-->
        <template id="registration_form" name="Registration">
            <t t-call="website.layout">
                <div class="container-fluid registration_body wow fadeIn" data-wow-offset="50"
                     data-wow-delay="0.9s" >
                    <div class="col-xs-12 col-md-7 reg_form_container">
                        <div class="panel reg_form_panel">
                            <div class="panel-heading text-center"><h3><i class="fa fa-user-plus" aria-hidden="true"></i><strong>  User Registration</strong></h3></div>
                            <div class="panel-body">
                                <form class="form-horizontal" enctype="multipart/form-data" t-attf-action="/process{{ '?debug' if debug else '' }}" method="post" onsubmit="this.action = this.action + location.hash" id="registration_form_val">
                                    <fieldset>
                                        <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">Company Name *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-building"></i></span>
                                                            <input  name="company_name" t-att-value="company_name" placeholder="Company Name" class="form-control one" style="text-transform:capitalize" type="text"  required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">Company Registration Number *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-building"></i></span>
                                                            <input name="company_reg_num" t-att-value="company_reg_num" placeholder="Company Registration Number" class="form-control one" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">Company Type</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select name="company_type1" class="form-control selectpicker" required="True">
                                                                <option value="" selected="selected">Select Company Type</option>
                                                                <t t-foreach="company_rec" t-as="company_type">
                                                                    <option name="company_type" t-if="company_type.id" t-att-value="company_type.id"><t t-esc="company_type.name"/></option>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">VAT NO *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input name="vat_no" placeholder="VAT NO" t-att-value="vat_no" class="form-control" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">First Name *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                            <input name="first_name" placeholder="First Name" t-att-value="first_name" class="form-control" style="text-transform:capitalize" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">Last Name *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                            <input name="last_name" placeholder="Last Name" t-att-value="last_name" class="form-control" style="text-transform:capitalize" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12 ">
                                                <div class="form-group">
                                                    <label class="control-label">E-Mail *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                                            <input id="email" name="email" placeholder="E-Mail Address" class="form-control" t-att-value="email" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                    <small id="email_check" class="help-block" style="color:#a94442;"></small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">Address 1*</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-home"></i></span>
                                                            <input name="street1" placeholder="Street 1" t-att-value="street1" class="form-control" style="text-transform:capitalize" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">Address 2 *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-home"></i></span>
                                                            <input name="street2" placeholder="Street 2" t-att-value="street2" class="form-control" style="text-transform:capitalize" type="text"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label class="control-label">Country *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select id="countries_phone1" class="form-control bfh-countries" data-country="AE" name="country"></select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label">State *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select class="form-control bfh-states" data-country="countries_phone1" data-state="AZ" name="state"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--</div>-->
                                            <!--<div class="row">-->
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label">City *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-home"></i></span>
                                                            <input type="text" class="form-control" t-att-value="city" name="city" style="text-transform:capitalize"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Zip Code *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="txtNumber" name="zip" onkeypress="return isNumberKey(event)" t-att-value="zip_code" maxlength="6" placeholder="Zip Code" class="form-control" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--</div>-->
                                            <!--<div class="row">-->
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Contact No. *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-phone-square"></i></span>
                                                            <input type="text" class="form-control bfh-phone" data-country="countries_phone1" name="contact_no"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Business Type</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select name="business_type" class="form-control selectpicker" required="True">
                                                                <option value="" selected="selected">Select Business Type</option>
                                                                <option value="manufacturer" >Manufacturer</option>
                                                                <option value="importer" >Importer</option>
                                                                <option value="retailer" >Retailer</option>
                                                                <option value="distributor" >Distributor</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--</div>-->
                                            <!--<div class="row">-->
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Product Type *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="product_type" name="product_type" placeholder="Product Type" t-att-value="product_type" class="form-control" type="text" style="text-transform:capitalize" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">Store Type</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select name="store_id1" class="form-control selectpicker" required="True">
                                                                <option value="" selected="selected">Select Merchant Type</option>
                                                                <t t-foreach="store_ids" t-as="store">
                                                                    <option name="store_id" t-if="store.id" t-att-value="store.id"><t t-esc="store.store_name"/></option>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-md-6 ">
                                                <div class="form-group">
                                                    <label class="control-label">Merchant Type</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select name="merchant_id1" class="form-control selectpicker" required="True">
                                                                <option value="" selected="selected">Select Merchant Type</option>
                                                                <t t-foreach="merchant_ids" t-as="merchant">
                                                                    <option name="merchant_id" t-if="merchant.id" t-att-value="merchant.id"><t t-esc="merchant.name"/></option>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">Brands Allowed to Sell (Hold CTRL/Shift key and select the Brands) *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-list"></i></span>
                                                            <select name="brand_ids" class="form-control selectpicker" required="True" multiple="multiple">
                                                                <!--<option value="" selected="selected">Select Brands to Sell</option>-->
                                                                <t t-foreach="brand_rec" t-as="brand">
                                                                    <option name="brand_ids" t-if="brand.id" t-att-value="brand.id"><t t-esc="brand.name"/></option>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Average Selling Price *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="avg_selling_price" name="avg_selling_price" t-att-value="avg_selling_price" placeholder="Average Selling Price" class="form-control" type="number" min="1"  required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--</div>-->
                                            <!--<div class="row">-->
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Average Monthly Sales *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="avg_monthly_sales" name="avg_monthly_sales" t-att-value="avg_monthly_sales" placeholder="Average Monthly Sales" class="form-control" type="number" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Number of Outlets *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="num_of_outlets" name="num_of_outlets" maxlength="6" t-att-value="num_of_outlets" placeholder="Number of Outlets" class="form-control" type="number" min="1" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Number of Styles *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="num_of_styles" name="num_of_styles" t-att-value="num_of_styles" placeholder="Product Type" class="form-control" type="number" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <label class="control-label">Details Presence in other Ecommerce Platforms *</label>
                                                    <label class="control-label">(Hold CTRL/Shift key and select the Ecommerce Platforms)</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <select name="ecommerce_ids" class="form-control selectpicker" required="True" multiple="multiple">
                                                                <!--<option value="" selected="selected">Select Ecommerce Platforms</option>-->
                                                                <t t-foreach="ecommerce_rec" t-as="ecommerce">
                                                                    <option name="ecommerce_ids" t-if="ecommerce.id" t-att-value="ecommerce.id"><t t-esc="ecommerce.name"/></option>
                                                                </t>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label  class="control-label">Type of Barcode Used *</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                                            <input id="types_of_barcodes" name="types_of_barcodes" t-att-value="types_of_barcodes" placeholder="base64" class="form-control" type="text" required="True"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12 ">
                                                <div class="form-group">
                                                    <label class="control-label">Domain</label>
                                                    <div class="inputGroupContainer">
                                                        <div class="input-group">
                                                            <input name="base_saas_domain_name" t-att-value="base_saas_domain_name" placeholder="Domain Name" class="form-control two"  type="text"  required="True"/>
                                                            <span class="input-group-addon">.<t t-raw="base_saas_domain"/></span>
                                                            <input type="hidden" name="base_saas_domain" t-att-value="base_saas_domain"/>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <label class="control-label"><h3>Upload KYC's</h3></label>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label  class="control-label">Emirates ID : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="emirates_id_card" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label  class="control-label">Passport and POA : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="passport_and_poa" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label  class="control-label">VAT Number : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="vat_num" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label  class="control-label">VISA and POA : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="visa_and_poa" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label class="control-label">Operating Address UAE  : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="operating_address_uae" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-md-12">
                                                <div class="form-group">
                                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                                        <span class="col-xs-12 col-md-4 text-left">
                                                            <label  class="control-label">Trade License : </label>
                                                        </span>
                                                        <span class="col-xs-12 col-md-8 text-left">
                                                            <input name="trade_license" type="file" data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true" accept="*"/>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
