<odoo>
    <data>
        <template id="login_assets_frontend" name="bista_login assets" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/css" href="/saas_user_restrict/static/src/css/style.css"/>
            </xpath>
        </template>

        <template id="suspended" name="suspended">
            <t t-call="website.layout">
                <div class="container-fluid suspended_template">
                    <section id="templates">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-12 text-center">
                                    <h2 class="section-heading">Sorry <i class="fa fa-exclamation" aria-hidden="true"></i></h2>
                                    <hr class="primary"/>
                                </div>
                            </div>
                        </div>
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-12 text-center">
                                    <div class="service-box">
                                        <i class="fa fa-10x fa-hourglass-end" aria-hidden="true"></i>
                                        <h2>Your account is suspended</h2>
                                        <h3>Please contact Administrator</h3>
                                        <h3> OR </h3>
                                        <p class="text-muted">
                                            <h3><a t-att-href="base_url" >
                                                Subscribe</a> </h3>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </t>
        </template>

